{
    "name": "ppfe",
    "version": "0.0.16",
    "description": "pp front-end integrated solution",
    "main": "index.js",
    "preferGlobal": true,
    "bin": {
        "ppfe": "bin/ppfe"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [ "fis", "pp" ],
    "repository": {
        "type": "git",
        "url": "https://github.com/qdsang/ppfe.git"
    },
    "dependencies": {
        "fis": "1.8.3",
        "fis-lint-jshint": "0.0.9",
        "fis-parser-marked": "0.0.5",
        "fis-parser-handlebars" : "0.0.1",
        "fis-optimizer-html-minifier": "0.0.9",
        "fis-parser-compass": "0.0.2",
        "fis-parser-jade": "0.0.12",
        "fis-parser-jade-inline": "*",
        "fis-parser-less": "0.1.1",
        "fis-parser-coffee-script": "0.0.2",
        "fis-parser-bdtmpl-chassis": "0.0.4",
        "fis-parser-utc": "0.0.2",
        "fis-parser-psd": "0.0.1",
        "fis-parser-po": "0.0.1",
        "fis-postprocessor-require-async": "*",
        "fis-postprocessor-region": "0.0.x",
        "fis-optimizer-html-minifier": "0.0.9",
        "fis-postpackager-simple": "0.0.23",
        "fis-postpackager-autoload": "0.0.13",
        "fis-deploy-default": "*",
        "fis-deploy-git": "*",
        "fis-command-xgettext2": "0.0.2"
    },
    "author": ["fouber"],
    "license": "MIT",
    "fisShortcut": {
        "dev": [
            "release", "--watch", "--clean", "--live", "--pack", "--dest", "dev"
        ],
        "test": [
            "release", "--clean", "--optimize", "--pack", "--md5", "--dest", "test"
        ],
        "stage": [
            "release", "--clean", "--optimize", "--pack", "--md5", "--dest", "stage"
        ],
        "release": [
            "release", "--clean", "--optimize", "--pack", "--md5", "--domains", "--dest", "release"
        ]
    }
}
